    "lodash": "^4.17.21",
    "marked": "^14.1.3",
    "path-browserify": "^1.0.1",
    "pinia": "^2.1.7",
    "python-shell": "^5.0.0",
    "sortablejs": "^1.15.2",
    "ts-morph": "^24.0.0",
    "uuid": "^9.0.1",
    "vite-plugin-string": "^1.2.3",
    "vite-raw-plugin": "^1.0.2",
    "vue-router": "^4.2.5",
    "vxe-pc-ui": "^4.2.53",
    "vxe-table": "4.9.4",
    "winston": "^3.17.0",
    "workerpool": "github:frankie-zeng/workerpool#24f9e85"
  },
  "devDependencies": {
