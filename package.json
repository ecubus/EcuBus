    "@iconify-icons/icon-park": "^1.2.11",
    "@iconify/icons-ep": "^1.2.12",
    "@iconify/icons-icon-park": "^1.2.11",
    "@iconify/icons-material-symbols": "^1.2.58",
    "@iconify/icons-ph": "^1.2.5",
