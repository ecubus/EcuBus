    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "electron-log": "^5.1.2",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.3.9",
    "element-plus": "^2.9.3",
    "emittery": "^1.0.3",
    "events": "^3.3.0",
    "glob": "^11.0.0",
    "handlebars": "^4.7.8",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.14.1",
